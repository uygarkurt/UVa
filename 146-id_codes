#define _CRT_SECURE_NO_WARNINGS
#include <algorithm>
#include <stdio.h>
#include <vector>
#include <iostream>
#include <string>
#include <iostream>
#include <sstream>
#include <cstring>
using namespace std;

int main()
{
	string s;
	while(cin>>s)
	{
		if(s=="#")
			break;
		if(next_permutation(s.begin(), s.end()))
			cout<<s<<"\n";
		else
			cout<<"No Successor"<<"\n";
	}
	return 0;
}
